@page
@model TrackItWeb.Pages.Nutrient.IndexModel
@{
	ViewData["Title"] = "fitness.";
}

<nav aria-label="breadcrumb">
	<ol class="breadcrumb">
		<li class="breadcrumb-item" aria-current="page"><a class="text-decoration-none text-dark" href="/Home/Index">home</a></li>
		<li class="breadcrumb-item active" aria-current="page">fitness</li>
	</ol>
</nav>

<div class="container-fluid p-3">
